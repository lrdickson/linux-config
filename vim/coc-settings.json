{
	"suggest.noselect": true,
	"diagnostic.displayByAle": true,
	"diagnostic.checkCurrentLine": false,
	"languageserver": {
		"nim": {
			"command": "nimlsp",
			"filetypes": ["nim"]
		},
		"csharp-ls": {
			"command": "csharp-ls",
			"filetypes": ["cs"],
			"rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"],
			"progressOnInitialization": false
		},
		"zig": {
			"command": "zls",
			"debugLog": true,
			"enabled": false,
			"filetypes": ["zig"],
			"path": "zls",
			"startUpMessage": true
		}
	}
}
